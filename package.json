{
  "name": "germane",
  "version": "0.0.0-development",
  "description": "A minimalistic time/date and timer library",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "start": "nodemon lib/index.js",
    "lint": "eslint .",
    "lint:fix": "prettier-eslint '**/*.js' --write",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn run lint:fix",
      "git add"
    ]
  },
  "keywords": [
    "date",
    "parse",
    "time",
    "dateTime",
    "timezone"
  ],
  "author": "Simeon Akpanudo (https://github.com/cmion)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "coveralls": "^3.0.6",
    "eslint": "^6.1.0",
    "eslint-utils": "1.4.2",
    "generator-code": "^1.2.6",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.8",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.31"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/germanejs/germane.git"
  },
  "bugs": {
    "url": "https://github.com/germanejs/germane/issues"
  },
  "homepage": "https://github.com/germanejs/germane#readme",
  "jest": {
    "collectCoverage": true
  }
}
